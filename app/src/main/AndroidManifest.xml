<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wtech.answeringcall">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".AdminActivity"
            android:label="@string/app_name">

            <intent-filter>
                <action android:name="com.wtech.answeringcall.ADMIN"/>
                <category android:name="android.intent.category.DEFAULT"/>

            </intent-filter>
        </activity>
        <activity
            android:name=".ResultAdminActivity"
            android:label="@string/app_name">

            <intent-filter>
                <action android:name="com.wtech.answeringcall.RESULTADMIN"/>
                <category android:name="android.intent.category.DEFAULT"/>

            </intent-filter>
        </activity>

        <activity
            android:name=".RegisterActivity"
            android:label="@string/app_name">

            <intent-filter>
                <action android:name="com.wtech.answeringcall.REGISTER"/>
                <category android:name="android.intent.category.DEFAULT"/>

            </intent-filter>
        </activity>

        <activity
            android:name=".CheckInActivity"
            android:label="@string/app_name">

            <intent-filter>
                <action android:name="com.wtech.answeringcall.CHECKIN"/>
                <category android:name="android.intent.category.DEFAULT"/>

            </intent-filter>
        </activity>

        <activity
            android:name=".ResultActivity"
            android:label="@string/app_name">

            <intent-filter>
                <action android:name="com.wtech.answeringcall.RESULT"/>
                <category android:name="android.intent.category.DEFAULT"/>

            </intent-filter>
        </activity><!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>


</manifest>